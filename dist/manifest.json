{
  "name": "Varjedag",
  "chrome_url_overrides": {
     "newtab": "newtab.html"
  },
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Learn a new word every time you open a new tab!",
  "icons": {
      "512": "logo512.png"
  },
  "browser_action": {
      "default_icon": "logo512.png",
      "default_popup": "popup.html"
  },
  "permissions": [ "tabs" ]
}